<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
         point="org.eclipse.recommenders.commons.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.rcp.codesearch.CodesearchModule">
      </module>
   </extension>
	<extension
         id="org.eclipse.recommenders.rcp.codesearch"
         name="Code Recommenders Codesearch Proposals"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <javaCompletionProposalComputer
            activate="true"
            categoryId="org.eclipse.recommenders.rcp.category.codesearch"
            class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.rcp.codesearch.completion.CodesearchCompletionProposalComputer">
      </javaCompletionProposalComputer>
   </extension>
<extension
      id="org.eclipse.recommenders.rcp.category.codesearch"
      name="Code Recommenders Codesearch"
      point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
   <proposalCategory
         icon="icons/obj16/search.png">
   </proposalCategory>
</extension>
<extension
      point="org.eclipse.ui.views">
   <view
         allowMultiple="false"
         category="org.eclipse.recommenders.rcp.category.views"
         class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.rcp.codesearch.views.QueryView"
         icon="icons/obj16/slocalhelp_obj.gif"
         id="org.eclipse.recommenders.internal.rcp.codesearch.views.QueryView"
         name="Codesearch Query"
         restorable="true">
   </view>
   <view
         category="org.eclipse.recommenders.rcp.category.views"
         class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.rcp.codesearch.views.ResultsView"
         icon="icons/obj16/slocalhelp_obj.gif"
         id="org.eclipse.recommenders.internal.rcp.codesearch.views.ResultsView"
         name="Codesearch Results"
         restorable="true">
   </view>
</extension>
	<extension
           point="org.eclipse.ui.preferencePages">
        <page
              class="org.eclipse.recommenders.internal.rcp.codesearch.preferences.CodesearchPreferencePage"
              id="org.eclipse.recommenders.rcp.category.codesearch.preferences"
              name="Codesearch"
              category="org.eclipse.recommenders.rcp.category.preferences" >
        </page>
     </extension>
     <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.recommenders.internal.rcp.codesearch.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
