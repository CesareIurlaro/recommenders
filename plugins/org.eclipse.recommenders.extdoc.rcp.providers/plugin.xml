<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.recommenders.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.extdoc.rcp.providers.ExtDocProvidersModule">
      </module>
   </extension>
   <extension
         point="org.eclipse.recommenders.rcp.extdoc.provider">
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.CallsProvider"
            long_name="Method Calls Provider"
            priority="15"
            short_name="Method Calls"
            icon="icons/full/obj16/calls.png">
      </provider>
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.ExamplesProvider"
            long_name="Code Examples Provider"
            priority="10"
            short_name="Code Examples"
            icon="icons/full/obj16/examples.gif">
      </provider>
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.SubclassingProvider"
            long_name="Subclassing Directives Provider"
            priority="5"
            short_name="Subclassing Directives"
            icon="icons/full/obj16/subclassing.gif">
      </provider>
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.SubclassingTemplatesProvider"
            long_name="Subclassing Patterns Provider"
            priority="2"
            short_name="Subclassing Patterns"
            icon="icons/full/obj16/subclassing-patterns.gif">
      </provider>
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.JavadocProvider"
            long_name="Javadoc Provider"
            priority="100"
            short_name="Javadoc"
            icon="icons/full/obj16/javadoc.gif">
      </provider>
      <!-- <provider
            class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.WikiProvider"
            long_name="Wiki Provider"
            priority="20"
            short_name="WikiDoc"
            icon="icons/full/obj16/wiki.gif">
      </provider> -->
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.extdoc.rcp.providers.SocialBookmarksProvider"
            long_name="Social Bookmarks Provider"
            priority="0"
            short_name="Social Bookmarks"
            icon="icons/full/obj16/socialbookmarks.gif">
      </provider>
   </extension>
</plugin>
