<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.recommenders.subwords.rcp"
         name="Subwords Completion Engine"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <javaCompletionProposalComputer
            activate="true"
            categoryId="org.eclipse.recommenders.subwords.rcp.category"
            class="org.eclipse.recommenders.internal.completion.rcp.subwords.SubwordsCompletionProposalComputer"
            needsSortingAfterFiltering="true"/>
   </extension>
	<extension
		point="org.eclipse.jdt.ui.javaCompletionProposalComputer"
		id="cr.JavadocHTMLCompletionProposalComputer_no">
		<javaCompletionProposalComputer 
			class="org.eclipse.jdt.internal.ui.text.javadoc.HTMLTagCompletionProposalComputer"
            categoryId="org.eclipse.recommenders.subwords.rcp.category">
			<partition type="__java_javadoc"/>
		</javaCompletionProposalComputer>
	</extension>
	<extension
		point="org.eclipse.jdt.ui.javaCompletionProposalComputer"
		id="cr.LegacyJavadocCompletionProposalComputer_all">
		<javaCompletionProposalComputer 
			class="org.eclipse.jdt.internal.ui.text.javadoc.LegacyJavadocCompletionProposalComputer"
			categoryId="org.eclipse.recommenders.subwords.rcp.category">
			<partition type="__java_javadoc"/>
		</javaCompletionProposalComputer>
	</extension>
 <extension
         id="org.eclipse.recommenders.subwords.rcp.category"
         name="Subwords Completion"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <proposalCategory
            icon="icons/elcl16/category.gif"></proposalCategory>
   </extension>
</plugin>
