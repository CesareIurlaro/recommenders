<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.eclipse.recommenders.rcp.extdoc.provider" name="org.eclipse.recommenders.rcp.extdoc.provider" schema="schema/org.eclipse.recommenders.rcp.extdoc.provider.exsd"/>
 <extension
         point="org.eclipse.recommenders.commons.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.rcp.extdoc.ExtDocModule">
      </module>
   </extension>
	<extension
	      point="org.eclipse.ui.views">
	   <view
	         allowMultiple="false"
	         category="org.eclipse.recommenders.rcp.category.views"
	         class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.rcp.extdoc.views.ExtDocView"
	         icon="$nl$/icons/full/eview16/extdoc.png"
	         id="org.eclipse.recommenders.internal.rcp.extdoc.views.ExtDocView"
	         name="ExtDoc"
	         restorable="true">
	   </view>
	</extension>
	<extension
           point="org.eclipse.ui.preferencePages">
        <page
              class="org.eclipse.recommenders.internal.rcp.extdoc.preferences.ExtDocPreferencePage"
              id="org.eclipse.recommenders.rcp.category.extdoc.preferences"
              name="ExtDoc"
              category="org.eclipse.recommenders.rcp.category.preferences" >
        </page>
     </extension>
     <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.recommenders.internal.rcp.extdoc.preferences.PreferenceInitializer">
      </initializer>
   </extension>
     <extension
           point="org.eclipse.recommenders.commons.selection.listener">
        <listener
              class="org.eclipse.recommenders.commons.injection.ExtensionFactory:org.eclipse.recommenders.internal.rcp.extdoc.UIManager">
        </listener>
     </extension>
</plugin>
